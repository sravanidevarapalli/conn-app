	<flow name="object-storeFlow" doc:id="7d234efb-ee59-4a34-abc2-7a614841132d" >
		<http:listener doc:name="Listener" doc:id="d1f1935d-054d-4d4a-8854-02af14132787" config-ref="HTTP_Listener_config" path="/obstore"/>
