		<os:store doc:name="Store" doc:id="63734184-801f-495d-ad0c-a6b4d8446d79" key="Name" objectStore="Object_store">
			<os:value ><![CDATA[#[payload.name]]]></os:value>
		</os:store>
		<logger level="INFO" doc:name="Logger" doc:id="7be29807-51ef-490d-85d8-2dbdfa433f25" message="#[payload]"/>
		<os:retrieve doc:name="Retrieve" doc:id="33bc79fd-57af-4157-a6f2-0952eb10022e" key="Name" objectStore="Object_store">
			<os:default-value ><![CDATA[0]]></os:default-value>
		</os:retrieve>
