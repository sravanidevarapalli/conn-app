		<logger level="INFO" doc:name="Logger" doc:id="488bfe31-2357-4c36-9335-4cd8a9034142" message="#[payload]"/>
		<os:retrieve-all doc:name="Retrieve all" doc:id="2f081c48-f03f-4c8f-bdeb-f0f92541ad0c" objectStore="Object_store"/>
